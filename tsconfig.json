{
	"compilerOptions": {
		"target": "ES2015",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"declaration": true,
		"outDir": "./dist",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true
	}
}
